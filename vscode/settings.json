  "css.validate": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": false
  },
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Victor Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.guides.highlightActiveIndentation": false,
  "editor.guides.indentation": false,
  "editor.linkedEditing": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.smoothScrolling": true,
  "editor.suggest.shareSuggestSelections": false,
  "editor.suggestSelection": "first",
  "editor.trimAutoWhitespace": true,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Comment",
        "scope": [
          "comment",
          "storage.type.js",
          "storage.type.ts",
          "storage.type.type.ts",
          "entity.name.function.js",
          "entity.name.function.ts",
          "keyword.control.module.js",
          "keyword.control.module.ts",
          "keyword.control.flow.tsx",
          "entity.other.attribute-name.class.css",
          "variable.other.readwrite.js",
          "variable.other.readwrite.ts",
          "variable.other.readwrite.shorthandpropertyname.js",
          "variable.other.readwrite.shorthandpropertyname.ts",
          "support.class.component.tsx",
          "string.quoted.single.tsx",
          "string.quoted.single.ts",
          "variable.other.readwrite.tsx",
          "string.quoted.double.tsx",
          "string.quoted.double.ts",
          "variable.other.constant.tsx",
          "variable.other.object.tsx",
          "variable.other.object.property.tsx",
          "variable.object.property.ts",
          "variable.other.property.tsx",
          "storage.type.tsx"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "emmet.extensionsPath": ["~"],
  "emmet.includeLanguages": {
    "html": "html",
    "css": "css",
    "javascript": "javascriptreact",
    "scss": "scss",
    "plaintext": "jade",
    "dockerfile": "dockerfile",
    "json": "json",
    "markdown": "html",
    "ruby": "ruby",
    "python": "python",
    "sql": "sql",
    "typescript": "typescriptreact",
    "yaml": "yaml",
    "erb": "html"
  },
  "emmet.excludeLanguages": [],
  "emmet.variables": {
    "lang": "en-US",
    "charset": "UTF-8"
  },
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.packageManager": "npm",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "Dockerfile-prod": "dockerfile",
    "dev-docker-compose.yml": "docker-compose",
    "*.js": "javascript",
    ".functions": "shellscript",
    ".aliases": "shellscript",
    "*.dev": "dockerfile",
    "Dockerfile-dev": "dockerfile"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": false,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/errorScreenshots": true,
    "**/node_modules": false,
    "**/mock": false,
    "**/.github": false,
    "**/lib": false
  },
  "files.insertFinalNewline": false,
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "gitlens.advanced.messages": {
    "suppressRebaseSwitchToTextWarning": true
  },
  "gitlens.statusBar.enabled": false,
  "html.autoClosingTags": true,
  "html.format.wrapAttributes": "force-aligned",
  "javascript.autoClosingTags": true,
  "javascript.suggest.autoImports": true,
  "javascript.suggest.completeJSDocs": true,
  "javascript.suggest.paths": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.preview.fontSize": 12,
  "scss.validate": false,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "svelte.enable-ts-plugin": true,
  "telemetry.telemetryLevel": "off",
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "kitty.app",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontFamily": "Victor Mono",
  "terminal.integrated.fontWeight": 700,
  "typescript.autoClosingTags": true,
  "typescript.suggest.autoImports": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.autoDetectColorScheme": true,
  "window.nativeTabs": true,
  "window.title": "${dirty} ${activeEditorShort}${separator}${rootName}",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Andromeda Italic",
  "workbench.preferredDarkColorTheme": "Activate UMBRA protocol",
  "workbench.preferredLightColorTheme": "Solarized Light",
  "workbench.iconTheme": "vscode-icons"
}
