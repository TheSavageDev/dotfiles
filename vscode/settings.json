{
  // UI Shrink
  "breadcrumbs.enabled": false,
  "editor.codeLens": false,
  "editor.folding": false, // Disables code folding making the gutter smaller
  "editor.glyphMargin": false, // Makes gutter smaller.
  "editor.hideCursorInOverviewRuler": false, // Removes cursor location on scrollbar.
  "editor.minimap.enabled": false, // Disables minimap
  "editor.overviewRulerBorder": false, // Removes border from overview ruler
  "editor.renderControlCharacters": false,
  "editor.renderLineHighlight": "none", // Removes line highlight
  "editor.renderWhitespace": "none",
  "editor.scrollbar.horizontal": "hidden", // Hides Horizontal Scrollbar
  "editor.scrollbar.vertical": "hidden", // Hides Vertical Scrollbar
  "explorer.openEditors.visible": 0, // removes the open editors section at the top of the sidebar, you can see the opened files with ⌘ + ⌥ + Tab
  "editor.occurrencesHighlight": false, // removes highlights occurrences (still works when you select a word)
  "editor.matchBrackets": "near", // removes the highlight of matching brackets
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "editorOverviewRuler.addedForeground": "#0000",
    "editorOverviewRuler.modifiedForeground": "#0000",
    "editorOverviewRuler.deletedForeground": "#0000"
  },
  "workbench.editor.showIcons": false, // removes icon from opened files in tabs
  "workbench.editor.showTabs": false, // ctl + tab
  "workbench.editor.tabCloseButton": "off", // removes cross icon from tabs
  "workbench.editor.tabSizing": "shrink",
  "workbench.statusBar.visible": true, // removes the status bar
  "workbench.tree.renderIndentGuides": "none", // Explorer indent guides
  "window.zoomLevel": -1,
  // End UI Shrink
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "color-highlight.markerType": "background",
  "color-highlight.markRuler": false,
